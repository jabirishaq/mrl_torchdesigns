@model FoxNetSoft.Plugin.Feed.Manager.Models.FeedRecModel
@using FoxNetSoft.Plugin.Feed.Manager.Models   
@using Nop.Web.Framework
@{
    Layout = "~/Administration/Views/Shared/_AdminLayout.cshtml";
    //page title
    ViewBag.Title = @T("Admin.FoxNetSoft.Plugin.Feed.Manager.Feed.AddNew").Text;
}
@using (Html.BeginForm(null, null, FormMethod.Post, new { id = "feed-form" }))
{
    <div class="section-header">
        <div class="title">
            <img src="@Url.Content("~/Administration/Content/images/ico-catalog.png")" alt="" />
            @T("Admin.FoxNetSoft.Plugin.Feed.Manager.Feed.AddNew") @Html.ActionLink("(" + T("Admin.FoxNetSoft.Plugin.Feed.Manager.Feed.BackToList") + ")", "List")
        </div>
        <div class="options">
            <button type="submit" name="save" value="save" class="k-button">@T("Admin.Common.Save")</button>
            <button type="submit" name="save-continue" value="save-continue" class="k-button">@T("Admin.Common.SaveContinue")</button>
        </div>
    </div>
    @Html.Partial(String.Format(Model.ViewPath,"_CreateOrUpdate"), Model) 
}
