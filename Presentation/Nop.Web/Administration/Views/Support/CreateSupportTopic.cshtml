@model Nop.Admin.Models.Support.SupportTopicModel
@{    
    //page title
    ViewBag.Title = T("Admin.Support.SupportTopics.AddNew").Text;
}
@using (Html.BeginForm())
{
    <div class="section-header">
        <div class="title">
            <img src="@Url.Content("~/Administration/Content/images/ico-content.png")" alt="" />
            @T("Admin.Support.SupportTopics.AddNew") @Html.ActionLink("(" + T("Admin.Support.SupportTopics.BackToList") + ")", "SupportTopicList")
        </div>
        <div class="options">
            <input type="submit" name="save" class="k-button" value="@T("Admin.Common.Save")" />
            <input type="submit" name="save-continue" class="k-button" value="@T("Admin.Common.SaveContinue")" />
        </div>
    </div>
    @Html.Partial("_CreateOrUpdateSupportTopic", Model)
}