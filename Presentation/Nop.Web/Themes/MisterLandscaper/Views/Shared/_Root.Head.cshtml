    </noscript>
    <!-- end Pinterest Tag -->
</head>
<body>
    @RenderBody()
    <script>
      
    $('option').click(function () {
        $('#select').blur();
    });
   
    $(document).ready(function () {
      
        var select = $('select');
        $('select').each(function () {
            var newDiv = $('<div />').addClass('action-select');
            $(this).before(newDiv);
            newDiv.append(this)
        });
       
        // for display search content at bellow
        var result = $(".megasearchresult").html();
        $(".forsearchresult").html(result);
        $(".megasearchresult").html('');

       
    });

    </script>

    @Html.NopCssFiles(this.Url, ResourceLocation.Foot)
    @Html.NopScripts(this.Url, ResourceLocation.Foot)
</body>

</html>
